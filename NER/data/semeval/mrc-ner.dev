	"1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition/paragraph_16": {
		"id": "1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition/paragraph_16",
		"phase": "train",
		"topic": "cs.ai",
		"document": "1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition",
		"paragraph": "paragraph_16",
		"prefix": "selected/train/cs.ai-ann0/1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition/paragraph_16",
		"text": "In the experiment , we first loaded $60$ midi files of drum tracks of \\textit{Metallica} and quantised them . Then they were encoded into the above described binary representation . We also added a flag \\texttt{\\char`_BAR\\char`_} as an annotation of the bar segments in order to check if the networks learns the local structure .",
		"entity": {},
		"relation": {}
	},
	"1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition/paragraph_14": {
		"id": "1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition/paragraph_14",
		"phase": "train",
		"topic": "cs.ai",
		"document": "1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition",
		"paragraph": "paragraph_14",
		"prefix": "selected/train/cs.ai-ann0/1604.05358v1.Text_based_LSTM_networks_for_Automatic_Music_Composition/paragraph_14",
		"text": "( b ) \\caption{Chord progressions generated by char-RNN (a) and word-RNN (b). Bar symbols ( [unused10] ) are inserted for readability and repeated chords in each bar are omitted. }  \\label{table:chord_results}  \\end{center}  \\end{table}",
