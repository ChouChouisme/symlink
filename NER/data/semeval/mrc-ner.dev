	"1810.08542v1.Atomic_photoionization_cross_sections_beyond_the_electric_dipole_approximation/paragraph_17": {
		"id": "1810.08542v1.Atomic_photoionization_cross_sections_beyond_the_electric_dipole_approximation/paragraph_17",
		"phase": "train",
