	"2101.12306v1.New_Formulations_of_Ambiguous_Volatility_with_an_Application_to_Optimal_Dynamic_Contracting/paragraph_6": {
		"id": "2101.12306v1.New_Formulations_of_Ambiguous_Volatility_with_an_Application_to_Optimal_Dynamic_Contracting/paragraph_6",
		"phase": "train",
