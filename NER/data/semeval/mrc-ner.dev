	"1906.07428v2.An_electrostatic_in_line_charge_state_purification_system_for_multicharged_ions_in_the_kiloelectronvolt_energy_range/paragraph_0": {
		"id": "1906.07428v2.An_electrostatic_in_line_charge_state_purification_system_for_multicharged_ions_in_the_kiloelectronvolt_energy_range/paragraph_0",
