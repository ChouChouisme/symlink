	"1805.08190v3.Spiral_bandwidth_of_four_wave_mixing_in_Rb_vapour/paragraph_14": {
		"id": "1805.08190v3.Spiral_bandwidth_of_four_wave_mixing_in_Rb_vapour/paragraph_14",
		"phase": "train",
		"topic": "physics.atom_ph",
		"document": "1805.08190v3.Spiral_bandwidth_of_four_wave_mixing_in_Rb_vapour",
