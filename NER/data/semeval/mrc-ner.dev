	"2102.03038v2.Bounds_and_Heuristics_for_Multi_Product_Personalized_Pricing/paragraph_11": {
		"id": "2102.03038v2.Bounds_and_Heuristics_for_Multi_Product_Personalized_Pricing/paragraph_11",
		"phase": "train",
		"topic": "econ.th",
