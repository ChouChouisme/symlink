	"1808.04075v2.Random_field_theory_based_p_values_a_review_of_the_SPM_implementation/paragraph_43": {
		"id": "1808.04075v2.Random_field_theory_based_p_values_a_review_of_the_SPM_implementation/paragraph_43",
		"phase": "train",
