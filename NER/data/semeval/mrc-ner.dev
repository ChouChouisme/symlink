	"1805.08190v3.Spiral_bandwidth_of_four_wave_mixing_in_Rb_vapour/paragraph_0": {
		"id": "1805.08190v3.Spiral_bandwidth_of_four_wave_mixing_in_Rb_vapour/paragraph_0",
		"phase": "train",
