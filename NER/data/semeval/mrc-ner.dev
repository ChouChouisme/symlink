	"1808.01998v3.Hodge_Decomposition_of_the_wall_shear_stress_vector_fields_characterizing_biological_flows/paragraph_8": {
		"id": "1808.01998v3.Hodge_Decomposition_of_the_wall_shear_stress_vector_fields_characterizing_biological_flows/paragraph_8",
		"phase": "train",
