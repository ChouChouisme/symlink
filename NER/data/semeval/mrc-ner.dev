	"2101.07760v1.DKPRG_or_how_to_succeed_in_the_Kolkata_Paise_Restaurant_gamevia_TSP/paragraph_57": {
		"id": "2101.07760v1.DKPRG_or_how_to_succeed_in_the_Kolkata_Paise_Restaurant_gamevia_TSP/paragraph_57",
		"phase": "train",
