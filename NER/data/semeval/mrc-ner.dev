	"1805.10930v2.Dipolar_Exchange_Quantum_Logic_Gate_with_Polar_Molecules/paragraph_8": {
		"id": "1805.10930v2.Dipolar_Exchange_Quantum_Logic_Gate_with_Polar_Molecules/paragraph_8",
