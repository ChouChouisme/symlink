	"1512.04888v2.A_tunable_CW_UV_laser_with_35_kHz_absolute_frequency_instability_for_precision_spectroscopy_of_Sr_Rydberg_states/paragraph_22": {
		"id": "1512.04888v2.A_tunable_CW_UV_laser_with_35_kHz_absolute_frequency_instability_for_precision_spectroscopy_of_Sr_Rydberg_states/paragraph_22",
