	"2011.13846v2.Wishful_Thinking_Persuasion_and_Polarization/paragraph_34": {
		"id": "2011.13846v2.Wishful_Thinking_Persuasion_and_Polarization/paragraph_34",
		"phase": "train",
		"topic": "econ.th",
		"document": "2011.13846v2.Wishful_Thinking_Persuasion_and_Polarization",
		"paragraph": "paragraph_34",
		"prefix": "selected/train/econ.th-ann4/2011.13846v2.Wishful_Thinking_Persuasion_and_Polarization/paragraph_34",
		"text": "A voter $i$ with belief $\\eta_i\\in\\Delta(\\Omega)$ and partisan preferences $u_i$ votes for the proposal if and only if $\\langle\\eta_i,u_i\\rangle=\\sum_{\\omega\\in\\Omega}\\eta_i(\\omega)u_i^\\omega$ is greater than $0$ such that the optimal voting strategy of any voter is \\begin{equation*} a(\\eta_i,u_i)=\\left\\{\\begin{array}{cc} 1 & \\mathrm{if} \\quad \\langle\\eta_i,u_i\\rangle\\geq 0 \\\\ 0 & \\mathrm{if} \\quad \\langle\\eta_i,u_i\\rangle < 0 \\end{array}\\right., \\end{equation*} for any $\\eta_i\\in\\Delta(\\Omega)$ , $u_i\\in\\mathbb{R}^{|\\Omega|}$ .",
		"entity": {},
