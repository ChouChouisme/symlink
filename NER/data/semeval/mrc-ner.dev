	"1802.09266v1.Imaging_Electron_Dynamics_with_Ultrashort_Light_Pulses_A_Theory_Perspective/paragraph_20": {
		"id": "1802.09266v1.Imaging_Electron_Dynamics_with_Ultrashort_Light_Pulses_A_Theory_Perspective/paragraph_20",
