	"1509.04737v1.Continuous_generation_of_Rubidium_vapor_in_hollow_core_photonic_band_gap_fibers/paragraph_15": {
		"id": "1509.04737v1.Continuous_generation_of_Rubidium_vapor_in_hollow_core_photonic_band_gap_fibers/paragraph_15",
		"phase": "dev",
