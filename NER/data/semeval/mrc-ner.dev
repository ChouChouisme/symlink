	"1604.02336v2.Back_to_the_Basics_Bayesian_extensions_of_IRT_outperform_neural_networks_for_proficiency_estimation/paragraph_5": {
		"id": "1604.02336v2.Back_to_the_Basics_Bayesian_extensions_of_IRT_outperform_neural_networks_for_proficiency_estimation/paragraph_5",
		"phase": "train",
