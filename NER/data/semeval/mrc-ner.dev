	"2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_33": {
		"id": "2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_33",
		"phase": "train",
		"topic": "econ.th",
		"document": "2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams",
		"paragraph": "paragraph_33",
		"prefix": "selected/train/econ.th-ann5/2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_33",
		"text": "\\begin{proposition}  \\label{P2} Let $Y=\\{a, b, c, d, e, f, g\\}\\subset\\RR$ where $a<b<c<d<e<f<g$ . For every $x\\in X$ , define , \\begin{equation*} N(x)=\\{n: n\\in \\N, x_{n}=a\\; \\text{or}\\; x_n=g\\}\\;\\text{and}\\; M(x)=\\{n: n\\in \\N, x_{n}=b\\; \\text{or}\\; x_n=f\\}. \\end{equation*} Define the social welfare function $W:X\\rightarrow \\RR$ by : \\begin{equation} \\label{P2E1} W(x)= \\begin{cases} -\\underset{n\\in N(x)}{\\sum}\\frac{1}{2^{n}} - \\underset{n\\in M(x)}{\\sum}\\frac{1}{3^{n}}&\\; \\text{if}\\; N(x)\\; \\text {or}\\; M(x) \\;\\text{is non-empty},\\\\ 0& \\; \\text{otherwise}. \\end{cases} \\end{equation} Then $W$ satisfies GE . \\end{proposition}",
		"entity": {},
		"relation": {}
	},
	"2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_32": {
		"id": "2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_32",
		"phase": "train",
		"topic": "econ.th",
		"document": "2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams",
		"paragraph": "paragraph_32",
		"prefix": "selected/train/econ.th-ann5/2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_32",
		"text": "\\begin{proposition}  \\label{P1} Let $Y\\equiv \\{a, b, c, d, e\\}\\subset\\RR$ be such that $a<b<c<d<e$ and let $X\\equiv Y^{\\N}$ . For any sequence $x\\in X$ , define : % \\begin{equation*} N(x) = \\{n:n\\in \\N\\;\\text{and}\\;x_{n}=a\\},\\;\\text{and}\\;M(x) = \\{m:m\\in \\N\\;\\text{and}\\;x_{m}=b\\}. \\end{equation*} % Define the social welfare function $W:X\\rightarrow \\RR$ by \\begin{equation}\\label{P1E1} W(x)=% \\begin{cases} -\\underset{n\\in N(x)}{\\sum}\\frac{1}{2^{n}} - \\underset{m\\in M(x)}{\\sum}\\frac{1}{3^{n}} & \\;\\text{if}\\;N(x)\\;\\text{or}\\;M(x)\\;\\text{is non-empty} \\\\ \\overset{\\infty}{\\underset{n=1}{\\sum}}\\frac{x_{n}}{2^{n}} & \\;\\text{otherwise}.% \\end{cases} \\end{equation} % Then $W(x)$ satisfies GE and M . % \\end{proposition}",
		"entity": {},
		"relation": {}
	},
	"2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_26": {
		"id": "2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_26",
		"phase": "train",
		"topic": "econ.th",
		"document": "2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams",
		"paragraph": "paragraph_26",
		"prefix": "selected/train/econ.th-ann5/2012.06481v2.Equitable_preference_relations_on_infinite_utility_streams/paragraph_26",
		"text": "[unused10] \\ emph { ( Infinite Equity - IE ) . Given $x$ , $y \\in X$ if there is $\\alpha \\in \\Pi$ such that $|\\dom(\\alpha)|=\\infty$ and for every $i \\in \\dom(\\alpha)$ one has either $y_i < x_i < x_{\\alpha(i)} < y_{\\alpha(i)}$ or $y_{\\alpha(i)} < x_{\\alpha(i)} < x_i < y_i$ and for all $k \\notin \\dom(\\alpha)$ one has $x_k = y_k$ , then $y \\prec x$ . } \\end{definition}",
		"entity": {},
