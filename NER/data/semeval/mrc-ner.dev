	"1601.00626v1.Scalable_Models_for_Computing_Hierarchies_in_Information_Networks/paragraph_56": {
		"id": "1601.00626v1.Scalable_Models_for_Computing_Hierarchies_in_Information_Networks/paragraph_56",
