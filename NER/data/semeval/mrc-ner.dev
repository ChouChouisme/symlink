	"1811.00668v1.Coherently_displaced_oscillator_quantum_states_of_a_single_trapped_atom/paragraph_7": {
		"id": "1811.00668v1.Coherently_displaced_oscillator_quantum_states_of_a_single_trapped_atom/paragraph_7",
		"phase": "train",
