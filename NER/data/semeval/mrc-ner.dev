	"1805.06931v1.Mechanisms_for_carbon_adsorption_on_Au_110_2_1_A_work_function_analysis/paragraph_12": {
		"id": "1805.06931v1.Mechanisms_for_carbon_adsorption_on_Au_110_2_1_A_work_function_analysis/paragraph_12",
		"phase": "train",
		"topic": "physics.atom_ph",
